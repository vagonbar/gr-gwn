<?xml version="1.0"?>
<block>
  <name>Event to PDU</name>
  <key>gwn_ev_to_pdu</key>
  <category>GWN</category>
  <import>import gwn</import>
  <make>gwn.ev_to_pdu($blkname, $blkid)</make>

  <param>
    <name>blkname</name>
    <key>blkname</key>
    <type>string</type>
  </param>
  <param>
    <name>blkid</name>
    <key>blkid</key>
    <type>string</type>
  </param>


  <sink>
    <name>in0</name>
    <type>message</type>
    <optional>1</optional>
  </sink>

  <source>
    <name>pdu</name>
    <type>message</type>
    <optional>1</optional>
  </source>

  <doc></doc>
</block>

