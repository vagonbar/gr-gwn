<?xml version="1.0"?>
<!--
###################################################
## Message Sender
###################################################
 -->
<block>
  <name>msg_sender_m</name>
  <key>gwn_msg_sender_m</key>
  <category>GWN</category>
  <import>import gwn</import>
  <make>gwn.msg_sender_m($blkname, $blkid)</make>

  <param>
    <name>blkname</name>
    <key>blkname</key>
    <type>string</type>
  </param>
  <param>
    <name>blkid</name>
    <key>blkid</key>
    <type>string</type>
  </param>

  <param>
    <name>interrupt</name>
    <key>interrupt</key>
    <type>int</type>
  </param>

  <param>
    <name>interval</name>
    <key>interval</key>
    <type>float</type>
  </param>

  <param>
    <name>retry</name>
    <key>retry</key>
    <type>int</type>
  </param>

  <param>
    <name>nickname1</name>
    <key>nickname1</key>
    <type>string</type>
  </param>

  <param>
    <name>nickname2</name>
    <key>nickname2</key>
    <type>string</type>
  </param>

	<source>
		<name>out0</name>
		<type>message</type>
		<optional>1</optional>
	</source>
	<source>
		<name>out1</name>
		<type>message</type>
		<optional>1</optional>
	</source>
</block>

