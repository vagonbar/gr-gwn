<?xml version="1.0"?>
<!--
###################################################
## Message Receiver
###################################################
 -->
<block>
  <name>msg_receiver_m</name>
  <key>gwn_msg_receiver_m</key>
  <category>GWN</category>
  <import>import gwn</import>
  <make>gwn.msg_receiver_m($blkname, $blkid)</make>

  <param>
    <name>blkname</name>
    <key>blkname</key>
    <type>string</type>
  </param>
  <param>
    <name>blkid</name>
    <key>blkid</key>
    <type>string</type>
  </param>

	<sink>
		<name>in0</name>
		<type>message</type>
		<optional>1</optional>
	</sink>
</block>

