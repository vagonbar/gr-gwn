<?xml version="1.0"?>
<block>
  <name>Data source</name>
  <key>gwn_data_source</key>
  <category>GWN</category>
  <import>import gwn</import>
  <make>gwn.data_source($blkname, $blkid, $interrupt, $interval, $retry, $nickname, $payload)</make>

  <param>
    <name>Block Name</name>
    <key>blkname</key>
    <type>string</type>
  </param>

  <param>
    <name>Block Id</name>
    <key>blkid</key>
    <type>string</type>
  </param>

  <param>
    <name>Interrupt</name>
    <key>interrupt</key>
    <value>False</value>
    <type>bool</type>
  </param>

  <param>
    <name>Interval</name>
    <key>interval</key>
    <value>1.0</value>
    <type>float</type>
  </param>

  <param>
    <name>Retry</name>
    <key>retry</key>
    <value>10</value>
    <type>int</type>
  </param>

  <param>
    <name>Data nickname</name>
    <key>nickname</key>
    <value>'DataData'</value>
    <type>string</type>
  </param>

<!--
  <param>
    <name>Event Dict</name>
    <key>ev_dict</key>
    <value>{}</value>
    <type>dict</type>
  </param>
-->

  <param>
    <name>Payload</name>
    <key>payload</key>
    <value>''</value>
    <type>string</type>
  </param>

  <!-- event source nodes -->
  <source>
    <name>out0</name>
    <type>message</type>
    <optional>1</optional>
  </source>

</block>

