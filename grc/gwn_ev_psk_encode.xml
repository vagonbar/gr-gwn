<?xml version="1.0"?>
<block>
  <name>Event PSK encode</name>
  <key>gwn_ev_psk_encode</key>
  <category>GWN</category>
  <import>import gwn</import>
  <make>gwn.ev_psk_encode($in_type, $debug)</make>

  <param>
    <name>in_type</name>
    <key>in_type</key>
    <type>enum</type>
    <option>
          <name>event</name>
          <key>'event'</key>
      </option>
    <option>
          <name>payload</name>
          <key>'payload'</key>
      </option>
      <option>
          <name>message</name>
          <key>'message'</key>
      </option>
  </param>
  <param>
    <name>Debug</name>
    <key>debug</key>
    <value>False</value>
    <type>bool</type>
  </param>

  <sink>
    <name>in0</name>
    <type>message</type>
    <optional>1</optional>
  </sink>

  <source>
    <name>pdu</name>
    <type>message</type>
    <optional>1</optional>
  </source>

  <doc>DEPRECATED, use L1 Framer instead</doc>

</block>
