<?xml version="1.0"?>
<block>
  <name>Virtual Channel</name>
  <key>gwn_virtual_channel</key>
  <category>GWN</category>
  <import>import gwn</import>
  <make>gwn.virtual_channel($blkname, $blkid, $prob_loss)</make>

  <param>
    <name>blkname</name>
    <key>blkname</key>
    <type>string</type>
  </param>
  <param>
    <name>blkid</name>
    <key>blkid</key>
    <type>string</type>
  </param>
  <param>
    <name>Probability Loss</name>
    <key>prob_loss</key>
    <value> 0.0 </value>
    <type>float</type>
  </param>

  <!-- event sink node -->
  <sink>
    <name>in0</name>
    <type>message</type>
    <optional>1</optional>
  </sink>

  <!-- PDU sink node -->
  <sink>
    <name>pdu</name>
    <type>message</type>
    <optional>1</optional>
  </sink>

  <!-- event source node -->
  <source>
    <name>out0</name>
    <type>message</type>
    <optional>1</optional>
  </source>

  <!-- PDU source node -->
  <source>
    <name>pdu</name>
    <type>message</type>
    <optional>1</optional>
  </source>

  <doc></doc>

</block>
