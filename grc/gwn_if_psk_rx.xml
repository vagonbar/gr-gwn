<?xml version="1.0"?>
<block>
  <name>IF PSK Receive</name>
  <key>gwn_if_psk_rx</key>
  <category>GWN</category>
  <import>import gwn</import>
  <make>gwn.if_psk_rx($blkname, $blkid)</make>

  <param>
    <name>blkname</name>
    <key>blkname</key>
    <type>string</type>
  </param>
  <param>
    <name>blkid</name>
    <key>blkid</key>
    <type>string</type>
  </param>

  <sink>
    <name>pdu</name>
    <type>message</type>
    <optional>1</optional>
  </sink>

  <source>
    <name>msg</name>
    <type>message</type>
    <optional>1</optional>
  </source>

</block>
