<?xml version="1.0"?>
<block>
  <name>Timer Source</name>
  <key>gwn_timer_source</key>
  <category>GWN</category>
  <import>import gwn</import>
  <make>gwn.timer_source($blkname, $blkid, $interrupt, $interval, $retry, $nickname1, $nickname2)</make>

 <param>
    <name>blkname</name>
    <key>blkname</key>
    <type>string</type>
  </param>
  <param>
    <name>blkid</name>
    <key>blkid</key>
    <type>string</type>
  </param>
  <param>
    <name>interrupt</name>
    <key>interrupt</key>
    <value>False</value>
    <type>bool</type>
  </param>
  <param>
    <name>interval</name>
    <key>interval</key>
    <value>1.0</value>
    <type>float</type>
  </param>
  <param>
    <name>retry</name>
    <key>retry</key>
    <value>3</value>
    <type>int</type>
  </param>
  <param>
    <name>nickname1</name>
    <key>nickname1</key>
    <value>TimerTOR1</value>
    <type>string</type>
  </param>
  <param>
    <name>nickname2</name>
    <key>nickname2</key>
    <value>TimerTOR2</value>
    <type>string</type>
  </param>

  <source>
    <name>out0</name>
    <type>message</type>
    <optional>1</optional>
  </source>

  <doc></doc>
  
</block>
