<?xml version="1.0"?>
<block>
  <name>Timer Source</name>
  <key>gwn_timer_source</key>
  <category>GWN</category>
  <import>import gwn</import>
  <make>gwn.timer_source($interrupt, $interval, $retry, $debug, $ev_dc_1, $ev_dc_2)</make>

  <param>
    <name>interrupt</name>
    <key>interrupt</key>
    <value>False</value>
    <type>bool</type>
  </param>
  <param>
    <name>interval</name>
    <key>interval</key>
    <value>1.0</value>
    <type>float</type>
  </param>
  <param>
    <name>retry</name>
    <key>retry</key>
    <value>3</value>
    <type>int</type>
  </param>
  <param>
    <name>debug</name>
    <key>debug</key>
    <value>False</value>
    <type>bool</type>
  </param>

  <param>
    <name>ev_dc_1</name>
    <key>ev_dc_1</key>
    <value>{}</value>
    <type>raw</type>
  </param>
  <param>
    <name>ev_dc_2</name>
    <key>ev_dc_2</key>
    <value>{}</value>
    <type>raw</type>
  </param>

  <source>
    <name>out0</name>
    <type>message</type>
    <optional>1</optional>
  </source>

  <doc></doc>
  
</block>
